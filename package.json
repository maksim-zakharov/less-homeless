{
  "name": "less-homeless",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "node serve-script",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config server/src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./server/src/ormconfig.ts",
    "start:dev": "concurrently \"ng serve --port 4201\" \"npm run serve\"",
    "dev:ssr": "ng run less-homeless:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "prebuild:ssr": "ngcc",
    "build:ssr": "ng build --prod && ng run less-homeless:server:production",
    "prerender": "ng run less-homeless:prerender"
  },
  "private": true,
