{
  "name": "less-homeless",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server",
    "build": "npm run build:ssr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config server/src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./server/src/ormconfig.ts",
    "start:dev": "concurrently \"ng serve --port 4201\" \"npm run serve\"",
    "ssr:serve": "nodemon --watch src/ -e ts,html,scss --exec npm run serve",
    "serve": "node serve-script",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run less-homeless:server:production",
    "dev:ssr": "ng run universal-starter-v9:serve-ssr",
    "prerender": "ng run universal-starter-v9:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.6",
    "@angular/common": "~10.0.6",
    "@angular/compiler": "~10.0.6",
    "@angular/core": "~10.0.6",
    "@angular/forms": "~10.0.6",
    "@angular/platform-browser": "~10.0.6",
    "@angular/platform-browser-dynamic": "~10.0.6",
    "@angular/platform-server": "~10.0.6",
    "@angular/router": "~10.0.6",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/ng-universal": "^2.0.1",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^7.1.0",
    "@nguniversal/common": "^10.0.1",
    "@nguniversal/express-engine": "^8.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.0.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "csurf": "^1.11.0",
    "date-fns": "^2.0.0-beta.5",
    "easy-yandex-s3": "^1.1.2",
    "helmet": "^4.1.1",
    "jwt-decode": "^3.0.0",
    "ng-zorro-antd": "^9.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.25",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^1.18.11",
    "protractor": "~7.0.0",
    "read-excel-file": "^4.0.7",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "wait-on": "^3.2.0",
    "webpack-cli": "^3.1.0",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
